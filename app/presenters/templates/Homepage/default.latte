{* This is the welcome page, you can delete it *}

{block content}
<div id="banner">
	<h1 n:block=title>Congratulations!</h1>
</div>

<div id="content">
	<h2>You have successfully created your Nette Framework project.</h2>

	{if $user->loggedIn}
		<p>Přihlášeno jako {$user->identity->username}</p>
		<a n:href="Sign:out">Odhlásit</a>
	{else}
		<p>Nepřihlášeno</p>
		<a n:href="Sign:register">Register</a><br>
		<a n:href="Sign:in">Login</a>
	{/if}

</div>
{/block}
